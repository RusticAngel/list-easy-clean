{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/.well-known/assetlinks.json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        },
        {
            "source": "/app-ads.txt",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain"
                }
            ]
        }
    ]
}